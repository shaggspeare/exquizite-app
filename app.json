{
  "expo": {
    "name": "exquizite-app",
    "slug": "exquizite-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "exquiziteapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shaggspeare.exquiziteapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:exquizite.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/logo.png"
      },
      "package": "com.shaggspeare.exquiziteapp",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "exquizite.app",
              "pathPrefix": "/shared"
            },
            {
              "scheme": "exquiziteapp",
              "host": "shared"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1E5A7D",
          "statusBar": {
            "style": "light"
          },
          "dark": {
            "backgroundColor": "#0D2D3E",
            "statusBar": {
              "style": "light"
            }
          }
        }
      ],
      "expo-secure-store",
      "expo-localization",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0376ac0c-084a-4748-92b8-f6f25ab4ea21"
      }
    },
    "owner": "shaggspeare"
  }
}
